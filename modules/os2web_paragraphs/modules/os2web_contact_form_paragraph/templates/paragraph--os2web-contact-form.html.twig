{% set classes = [
  'paragraph',
  'paragraph--type--' ~ paragraph.bundle|clean_class,
  view_mode ? 'paragraph--view-mode--' ~ view_mode|clean_class,
  paragraph.isPublished() ? null : 'paragraph--unpublished'
] %}
{% block paragraph %}
  <div{{ attributes.addClass(classes) }}>
    {% block content %}

      {# Begin - heading #}
      {{ content.field_os2web_contact_form_head }}
      {# End - heading #}

      {# Begin - subheading #}
      {{ content.field_os2web_contact_form_subh }}
      {# End - subheading #}

      {# Begin - contact form #}
      {{ contact_form }}
      {# End - contact form #}

    {% endblock %}
  </div>
{% endblock paragraph %}
